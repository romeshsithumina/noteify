generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_CONNECTION_STRING")
}

model notes {
  id        String    @id @default(auto()) @map("_id") @db.ObjectId
  v         Int?      @map("__v")
  createdAt DateTime? @db.Date
  text      String
  title     String
  updatedAt DateTime? @db.Date
}
